{
  "name": "chemistli",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server/index.js",
  "scripts": {
    "start": "npm run build && node ./dist/server/index.js",
    "build": "node ./node_modules/webpack/bin/webpack --config webpack.prod.js && cross-env NODE_ENV=server babel ./src -d ./dist",
    "dev-server": "nodemon --watch ./src --exec \"cross-env NODE_ENV=server babel-node ./src/server/index.js\"",
    "dev": "concurrently \"npm run dev-server\" \"webpack --watch --config webpack.common.js\" ",
    "precommit": "lint-staged",
    "prettify": "prettier --single-quote --trailing-comma all --write \"src/**/*.js\""
  },
  "author": "Johannes Kronm√ºller <Johannes.Kronmueller@gmail.com>",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/johakr/chemistli"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --trailing-comma all --write",
      "git add"
    ]
  },
  "dependencies": {
    "es6-promise": "^4.0.5",
    "express": "^4.14.1",
    "helmet": "^3.4.1",
    "isomorphic-fetch": "^2.2.1",
    "linkstate": "^1.0.1",
    "milligram": "^1.3.0",
    "preact": "^8.1.0",
    "preact-render-to-string": "^3.6.0",
    "preact-router": "^2.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^7.0.0",
    "babel-minify": "^0.3.0",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-transform-react-jsx": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-stage-0": "^6.22.0",
    "concurrently": "^3.3.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.0",
    "eslint": "^4.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-inline-source-plugin": "^0.0.9",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.14.1",
    "lint-staged": "^6.0.0",
    "node-babel": "^0.1.2",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "prettier": "^1.3.1",
    "purify-css": "^1.2.5",
