{
  "name": "chemistli",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server/index.js",
  "scripts": {
    "start": "npm run build && node ./dist/server/index.js",
    "build": "webpack -p && cross-env NODE_ENV=server babel ./src -d ./dist",
    "dev-server": "nodemon --watch ./src --exec \"cross-env NODE_ENV=server babel-node ./src/server/index.js\"",
    "dev": "concurrently \"npm run dev-server\" \"webpack --watch -d\" ",
    "precommit": "lint-staged",
    "prettify": "prettier --single-quote --trailing-comma all --write \"src/**/*.js\""
  },
  "author": "Johannes Kronm√ºller <Johannes.Kronmueller@gmail.com>",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/johakr/chemistli"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --trailing-comma all --write",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "es6-promise": "^4.0.5",
    "express": "^4.14.1",
    "helmet": "^3.4.1",
    "isomorphic-fetch": "^2.2.1",
    "linkstate": "^1.0.1",
    "milligram": "^1.3.0",
    "preact": "^8.1.0",
    "preact-render-to-string": "^3.6.0",
    "preact-router": "^2.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-loader": "^8.0.0",
    "concurrently": "^4.0.0",
    "cross-env": "^5.0.1",
    "css-loader": "^1.0.0",
    "eslint": "^5.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.1",
    "lint-staged": "^7.1.0",
    "node-babel": "^0.1.2",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "prettier": "^1.13.7",
